  "k-angular-material-table": {
    "prefix": "k-angular-material-table\r",
    "body": [
      "\r",
      "<div class=\"w-full\">\r",
      "    <table class=\"!border w-full cursor-pointer\" mat-table [dataSource]=\"dataSource\" matSort>\r",
      "        @for (column of displayedColumns; track column) {\r",
      "          <ng-container [matColumnDef]=\"column\">\r",
      "            <th class=\"whitespace-nowrap\" mat-header-cell *matHeaderCellDef mat-sort-header>{{ ColumnName[column] }}\r",
      "            </th>\r",
      "            <td class=\"whitespace-nowrap\" mat-cell *matCellDef=\"let row;let idx = index\">\r",
      "                 {{ column === 'STT' ? idx + 1 : row[column] }}\r",
      "            </td>\r",
      "         </ng-container>\r",
      "        }\r",
      "        <tr mat-header-row *matHeaderRowDef=\"displayedColumns\"></tr>\r",
      "        <tr mat-row *matRowDef=\"let row; columns: displayedColumns;\" (click)=\"goToDetail(row);\"></tr>\r",
      "        <tr class=\"mat-row\" *matNoDataRow>\r",
      "            <td class=\"mat-cell\" colspan=\"4\">Không tìm thấy \"{{input.value}}\"</td>\r",
      "        </tr>\r",
      "    </table>\r",
      "    <mat-paginator [pageSizeOptions]=\"[5, 10, 25, 100]\" aria-label=\"Select page of giohangs\"></mat-paginator>\r",
      "</div>"
    ]
  }
}